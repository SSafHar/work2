{
  "name": "juan-pardo",
  "version": "0.0.0",
  "license": "MIT",
  "engines": {
    "yarn": ">=1.3.2",
    "node": "^8.9.4"
  },
  "scripts": {
    "postinstall": "yarn build",
    "ng": "ng",
    "gulp": "gulp",
    "concurrently": "concurrently",
    "build:icons": "gulp build:icons",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:client-bundle": "ng build --prod",
    "build:server-bundle": "ng build --prod --app 1 --output-hashing=false",
    "build:server": "gulp build:server",
    "build": "yarn build:client-bundle && yarn build:server-bundle && yarn build:server",
    "start": "node dist/backend/server.js",
    "watch:client-bundle": "ng build --delete-output-path=false --watch",
    "watch:server-bundle": "ng build --delete-output-path=false --aot --app 1 --output-hashing=false --watch",
    "watch:ng": "concurrently --kill-others 'yarn watch:client-bundle' 'yarn watch:server-bundle'",
    "watch:server": "gulp watch:server",
    "watch": "concurrently --kill-others 'yarn watch:ng' 'yarn watch:server'",
    "serve": "concurrently --kill-others 'yarn watch' 'yarn gulp serve'",
    "lint:styles": "gulp lint:styles"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.0",
    "@angular/cdk": "^5.2.4",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/material": "^5.2.4",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/platform-server": "^5.2.9",
    "@angular/router": "^5.2.0",
    "@nguniversal/express-engine": "^5.0.0-beta.6",
    "@nguniversal/module-map-ngfactory-loader": "^5.0.0-beta.6",
    "@ngx-translate/core": "^9.1.1",
    "@ngx-translate/http-loader": "^2.0.1",
    "@vimeo/player": "^2.3.1",
    "body-parser": "^1.18.2",
    "core-js": "^2.4.1",
    "express": "^4.16.3",
    "material-design-icons": "^3.0.1",
    "moment": "^2.21.0",
    "node-fetch": "^2.1.1",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^5.5.6",
    "ts-loader": "^4.0.1",
    "vimeo": "^2.0.1",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "1.6.7",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/language-service": "^5.2.0",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.1",
    "concurrently": "^3.5.1",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.4.0",
    "gulp-filter": "^5.1.0",
    "gulp-font-icons": "^0.1.39-beta",
    "gulp-iconfont": "^9.1.0",
    "gulp-nodemon": "^2.2.1",
    "gulp-sass-lint": "^1.3.4",
    "gulp-typescript": "^4.0.1",
    "gulp-wait": "^0.0.2",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
